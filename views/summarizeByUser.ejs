<h1>Summary of all Transaction</h1>
<table class="table table-bordered table-striped">
    <ul>
        <li><a href="/transaction">transaction table</a></li>
    </ul>
    <thead>
        <tr>
            <th>username</th>
            <th>age</th>
            <th>number of todos</th>
        </tr>
    </thead>
    <tbody>
        <% results.map(item => {%>
            <tr>
               <td> <%= item._id.username %> </td>
               <td><%= item._id.age %></td>
               <td><%= item.total %></td>
            </tr>
            
            <% }) %>

    </tbody>
</table>
